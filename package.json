{
  "name": "discord-rpc",
  "version": "0.1.2",
  "description": "",
  "dependencies": {
    "discord-rich-presence": "0.0.8",
    "systray": "^1.0.5",
    "ws": "^7.1.1"
  },
  "devDependencies": {
    "archiver": "^3.1.1",
    "create-nodew-exe": "^1.0.5",
    "pkg": "^4.4.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:win && npm run build:linux && npm run build:macos && npm run build:pack",
    "build:win": "npm run build:win:64 && npm run build:win:32",
    "build:win:64": "pkg -c Server/pkg/win.json --out-path ./dist -t node10-win-x64 Server/win_tray_server.js && create-nodew-exe ./dist/server_win_64bit_debug.exe ./dist/server_win_64bit.exe",
    "build:win:32": "pkg -o ./dist/server_win_32bit_debug.exe -t node10-win-x86 server.js && create-nodew-exe ./dist/server_win_32bit_debug.exe ./dist/server_win_32bit.exe",
    "build:linux": "npm run build:linux:64 && npm run build:linux:32",
    "build:linux:64": "pkg -c Server/pkg/linux.json --out-path ./dist -t node10-linux-x64 Server/linux_tray_server.js",
    "build:linux:32": "pkg -o ./dist/server_linux_no_tray_debug -t node10-linux-x64 server.js",
    "build:macos": "npm run build:macos:64 && npm run build:macos:32",
    "build:macos:64": "pkg -c Server/pkg/mac.json --out-path ./dist -t node10-macos-x64 Server/linux_tray_server.js",
    "build:macos:32": "pkg -o ./dist/server_macos_no_tray_debug -t node10-macos-x64 server.js",
    "build:pack": "node server/pkg/pack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lolamtisch/discord-rpc.git"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/lolamtisch/discord-rpc/issues"
  },
  "homepage": "https://github.com/lolamtisch/discord-rpc#readme"
}
